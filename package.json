{
  "name": "memebot",
  "version": "0.3.0",
  "engines": {
    "node": "8.9.1"
  },
  "main": "index.js",
  "scripts": {
    "precommit": "lint-staged",
    "start": "node server/index.js",
    "test": "jest --watch server/tests/worker.test.js",
    "heroku-postbuild": "cd react-ui/ && npm install && npm install --only=dev --no-shrinkwrap && npm run build"
  },
  "dependencies": {
    "amqplib": "^0.5.2",
    "aws-sdk": "^2.188.0",
    "ejs": "^2.5.6",
    "express": "^4.15.2",
    "pg": "^7.4.1"
  },
  "devDependencies": {
    "husky": "^0.14.3",
    "jest": "^22.2.1",
    "lint-staged": "^6.1.0",
    "prettier": "^1.10.2",
    "request": "^2.81.0",
    "tape": "^4.7.0"
  },
  "lint-staged": {
    "{react-ui/src,server}/**/*.{js,jsx,json,css}": [
      "prettier --single-quote --write",
      "git add"
    ]
  }
}
